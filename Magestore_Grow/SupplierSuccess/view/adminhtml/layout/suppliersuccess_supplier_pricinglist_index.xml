<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="editor"/>
    <update handle="styles"/>
    <head>
        <css src="Magestore_SupplierSuccess::css/supplier.css"/>
    </head>
    <body>
        <referenceContainer name="content">
            <uiComponent name="os_supplier_pricinglist_form"/>
        </referenceContainer>
        <referenceContainer name="os_supplier_pricinglist_form">
            <block  class="Magestore\SupplierSuccess\Block\Adminhtml\Supplier\Import\PricingList\Form" name="import-form"
                    template="Magestore_SupplierSuccess::import/form.phtml">
                <arguments>
                    <argument name="config" xsi:type="array">
                        <item name="label" xsi:type="string" translate="true"> </item>
                        <item name="collapsible" xsi:type="boolean">false</item>
                        <item name="opened" xsi:type="boolean">false</item>
                        <item name="sortOrder" xsi:type="string">22</item>
                        <item name="canShow" xsi:type="boolean">true</item>
                        <item name="componentType" xsi:type="string">fieldset</item>
                    </argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
